<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#000000"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/txtDate"
        style="@style/TextView.Value"
        android:layout_width="@dimen/change_product_print"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:background="#FFFFFF"
        android:text="09/06/2557" />

    <TextView
        android:id="@+id/txtHold_Cash"
        style="@style/TextView.Value"
        android:layout_width="@dimen/employee_header_height"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="1" />

    <TextView
        android:id="@+id/txtHold_Credit"
        style="@style/TextView.Value"
        android:layout_width="@dimen/employee_header_height"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="1" />

    <TextView
        android:id="@+id/txtCount_Install_Cash"
        style="@style/TextView.Value"
        android:layout_width="@dimen/employee_header_height"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="1" />

    <TextView
        android:id="@+id/txtCount_Install_Credit"
        style="@style/TextView.Value"
        android:layout_width="@dimen/employee_header_height"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="1" />

    <TextView
        android:id="@+id/txtSum_Count_Cash"
        style="@style/TextView.Value"
        android:layout_width="@dimen/employee_header_height"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="1" />

    <TextView
        android:id="@+id/txtSum_Count_Credit"
        style="@style/TextView.Value"
        android:layout_width="@dimen/employee_header_height"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="1" />

   
</LinearLayout>